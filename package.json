{
  "name": "todo-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --exec ts-node src/server.ts",
    "prod": "node ./dist/src/server.js",
    "build": "rm -rf ./dist && tsc --project tsconfig.json && node copyfiles -u 1 src/db/*.json dist/db",
    "build:ci": "tsc --project tsconfig.json && node copyfiles -u 1 src/db/*.json dist/db",
    "test:unit": "jest --silent",
    "prisma:gen": "npx prisma migrate dev --name init && npx prisma generate",
    "action:local": "act --secret-file=my.secrets --privileged --container-architecture=linux/amd64 push --container-daemon-socket -",
    "docker:build-db": "docker-compose -f docker-compose-db.yml up -d",
    "docker:build-app": "docker-compose -f docker-compose-app.yml up -d"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",

    "superagent": "^9.0.2",
    "supertest": "^7.0.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "@eslint/js": "^9.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/superagent": "^8.1.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.7.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
